            dct:description "RECOMMENDED. The fraction of partial Fourier information collected. Corresponds to DICOM Tag 0018, 9081 Partial Fourier."^^xsd:string ;

            owl:sameAs dicom:dicom_00189081 ;

            obo:IAO_0000136 nidm:k-spaceTraversalScheme, nidm:AcquisitionMethod, nidm:PulseSequence, nidm:MagneticResonanceImaging ;

